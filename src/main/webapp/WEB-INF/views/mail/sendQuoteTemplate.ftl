<title>Presupuesto realizado</title>
<link href="http://www.singuia.com/static/new/css/mail.css" rel="stylesheet">
</head>
<body bgcolor="#FFFFFF">
	<!-- HEADER -->
	<table class="head-wrap" bgcolor="#000000">
		<tr>
			<td></td>
			<td class="header container" >
					<div class="content">
					<table bgcolor="#000000">
						<tr>
							<td><img src="http://www.singuia.com/static/new/img/logos/mail-logo.png" /></td>
						</tr>
					</table>
					</div>
			</td>
			<td></td>
		</tr>
	</table>
	<!-- /HEADER -->
	
	<!-- BODY -->
	<table class="body-wrap">
		<tr>
			<td></td>
			<td class="container" bgcolor="#FFFFFF">
				<div class="content">
				<table>
					<tr>
						<td>
							<h3>Presupuesto realizado</h3>
							<p class="lead">
								El usuario ${firstName} ${lastName?substring(0, 1)} ha realizado un presupuesto para su pedido.
							</p>
							<p class="callout">
								Para acceder hacé click <a href="http://www.singuia.com/dashboard/requests">aquí</a>.
							</p>
							<!-- social & contact -->
							<table class="social" width="100%">
								<tr>
									<td>
										<!-- column 1 -->
										<table align="left" class="column">
											<tr>
												<td>
													<h5 class="">Seguinos:</h5>
													<p class=""><a href="https://www.facebook.com/SinGuia.Oficial" class="soc-btn fb">Facebook</a> <a href="https://twitter.com/singuia" class="soc-btn tw">Twitter</a></p>
												</td>
											</tr>
										</table>
										<!-- /column 1 -->
										<!-- column 2 -->
										<table align="left" class="column">
											<tr>
												<td>										
													<h5 class="">Contacto:</h5>												
													<p>Email: <strong><a href="emailto:contacto@singuia.com">contacto@singuia.com</a></strong></p>
												</td>
											</tr>
										</table>
										<!-- /column 2 -->
										<span class="clear"></span>	
									</td>
								</tr>
							</table>
							<!-- /social & contact -->
						</td>
					</tr>
				</table>
				</div>
				<!-- /content -->
			</td>
			<td></td>
		</tr>
	</table>
